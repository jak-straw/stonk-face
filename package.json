{
  "name": "stonk-face",
  "version": "1.0.0",
  "description": "A modern video sharing application for YouTube videos, built with React and Node.js",
  "main": "server/src/server.ts",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "tsx watch server/src/server.ts",
    "client:dev": "vite",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p server/tsconfig.json",
    "build:client": "vite build",
    "start": "node dist/server/server.js",
    "server:start": "node dist/server/server.js",
    "client:start": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "video",
    "youtube",
    "react",
    "nodejs",
    "express",
    "typescript",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mongodb": "^6.3.0",
